# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

PUT     /set                    controllers.CacheAPI.set()
GET		/get/:key				controllers.CacheAPI.get(key: String)

PUT     /setmc                  controllers.CacheAPI.setMemcached()
GET		/getmc/:key				controllers.CacheAPI.getMemcached(key: String)
